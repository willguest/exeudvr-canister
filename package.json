{
  "name": "exeudvr-canister",
  "private": true,
  "version": "0.1.0",
  "description": "Open VR on the Internet Computer",
  "scripts": {
    "check-dfx": "node src/scripts/check-dfx.js",
    "presetup": "sudo apt-get update && sudo apt-get install -y inotify-tools jq && run-s check-dfx",
    "setup": "npm i && dfx deps pull && dfx canister create backend && dfx generate backend && dfx canister create frontend && dfx generate frontend",
    "prebuild": "run-s unity",
    "build": "tsc && webpack",
    "unity": "nohup bash src/scripts/unity.sh > src/unity/unity_watch.log 2>&1 &",
    "prestart": "pkill -f 'bash src/scripts/unity.sh' &",
    "start": "dfx deps deploy && run-p backend frontend",
    "backend": "mo-dev --generate --deploy -y",
    "frontend": "webpack serve --mode development --env development",
    "test": "run-s test:backend",
    "test:backend": "",
    "format": "prettier --write \"src/**/*.{json,js,jsx,ts,tsx,css,scss}\" \"backend/**/*.mo\"",
    "sources": "mops sources",
    "postinstall": "mops install"
  },
  "devDependencies": {
    "@dfinity/agent": "^0.19.3",
    "@dfinity/auth-client": "^0.19.3",
    "@dfinity/candid": "^0.19.3",
    "@dfinity/nns": "^0.16.7",
    "@dfinity/principal": "^0.19.3",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@types/node": "^20.16.2",
    "@types/react": "^18.2.14",
    "@types/react-dom": "^18.2.6",
    "@types/webpack": "^5.28.3",
    "@vitejs/plugin-react": "^4.0.1",
    "assert": "^2.1.0",
    "buffer": "^6.0.3",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^11.0.0",
    "cross-fetch": "^3.1.6",
    "css-loader": "^6.8.1",
    "declaration-bundler-webpack-plugin": "^1.0.3",
    "dotenv": "^16.3.1",
    "events": "3.3.0",
    "html-webpack-plugin": "^5.5.3",
    "ic-mops": "^0.28.1",
    "jsdom": "^22.1.0",
    "mo-dev": "^0.11.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.8",
    "prettier-plugin-motoko": "^0.5.3",
    "process": "0.11.10",
    "stream-browserify": "3.0.0",
    "style-loader": "^3.3.3",
    "terser-webpack-plugin": "^5.3.3",
    "ts-loader": "^9.5.0",
    "ts-node": "^10.9.1",
    "ts-transformer-keys": "^0.4.4",
    "ttypescript": "^1.5.15",
    "typescript": "^5.2.2",
    "url-loader": "^4.1.1",
    "util": "0.12.5",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1",
    "wepack-cli": "^0.0.1-security"
  },
  "engines": {
    "node": "^12 || ^14 || ^16 || ^18 || ^20"
  },
  "browserslist": [
    "last 2 chrome version",
    "last 2 firefox version",
    "last 2 safari version",
    "last 2 edge version"
  ],
  "dependencies": {
    "@dfinity/identity": "^0.19.3",
    "cargo": "^0.8.0",
    "decompress": "^4.2.1",
    "decompress-unzip": "^4.0.1",
    "local-storage": "^2.0.0",
    "node": "^20.7.0",
    "node-fetch": "^3.3.2",
    "nvm": "^0.0.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-loading-icons": "^1.1.0",
    "react-router-dom": "^6.16.0",
    "react-unity-renderer": "^2020.2.0",
    "react-unity-webgl": "8.8.0",
    "redux": "^4.2.1",
    "rsync": "^0.6.1",
    "rustup": "^1.0.10",
    "styled-components": "^6.0.9",
    "wasm-opt": "^1.4.0"
  }
}
